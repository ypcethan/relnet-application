<%= csrf_meta_tags %>
<%= csp_meta_tag %>

<% if content_for?(:title) %>
  <title><%= yield(:title) %> | ReleaseNet </title>
<% else %>
  <title>ReleaseNet </title>
<% end %>

<%= stylesheet_link_tag    'application', media: 'all' %>
<%= javascript_include_tag 'application' %>
<meta name="viewport" content="width=device-width, initial-scale=1.0">
<link href="https://fonts.googleapis.com/css?family=Poppins:100,200,300,400,500,600" rel="stylesheet">
<%= stylesheet_link_tag 'https://cdnjs.cloudflare.com/ajax/libs/toastr.js/2.1.3/toastr.min.css' %>
<%= javascript_include_tag 'https://cdnjs.cloudflare.com/ajax/libs/toastr.js/2.1.3/toastr.min.js' %>
<%= javascript_include_tag 'https://unpkg.com/micromodal/dist/micromodal.min.js' %>
<script src="//cdnjs.cloudflare.com/ajax/libs/timepicker/1.3.5/jquery.timepicker.min.js"></script>
<link rel="stylesheet" href="//cdnjs.cloudflare.com/ajax/libs/timepicker/1.3.5/jquery.timepicker.min.css">
<%= favicon_link_tag asset_path('favicon.svg') %>
<%= render 'layouts/flash_rendering_toastr' %>

<script src="https://unpkg.com/popper.js@1"></script>
<script src="https://unpkg.com/tippy.js@4"></script>
